  x=$$("a[target='naf']")
  list = []; for(i=0;i<x.length;i++){list.push(x[i].getAttribute("href"))}

  list2=[]
  for item in list:
      list2.append((f"https://elaws.moj.gov.ae/{item}", "ae", 1))
  print(list2)
